    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.2/css/all.min.css" />
    <link rel="stylesheet" href="assets/css/estilo.css" />
    <title>VETERIANARIA</title>
</head>
<body>
    <!-- MENU ENCABEZADO -->
    {{> header}}

    <!-- SECCIÓN DE PROYECTOS -->
    <div class="proyectos">
        <div class="contenido-seccion">
            <h2>.</h2>
            <h2> VETERINARIA C++</h2>
            <div class="galeria">
                <!-- Proyecto 1 -->
                <div class="proyecto">
                    <img src="/images/VETERINARIA/1.jpg" alt="Proyecto 1">
                    <div class="overlay">
                        <h3>Proyecto 1</h3>
                        <p>AQUI PODEMOS OBSERVAR EL INICIO.</p>
                    </div>
                </div>
                <!-- Proyecto 2 -->
                <div class="proyecto">
                    <img src="/images/VETERINARIA/2.jpg" alt="Proyecto 2">
                    <div class="overlay">
                        <h3>Proyecto 2</h3>
